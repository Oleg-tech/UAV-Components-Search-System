{
  "shop_name": "MyDrone",
  "shop_file_name": "mydrone",
  "shop_logo": "https://rc-hobby.com.ua/upload/rc-hobby-2019.png",
  "search_url": "https://mydrone.com.ua/search/?search=REPLACE_WITH_QUERY",
  "base_url": "https://mydrone.com.ua",
  "query_separator": " ",
  "headers": {
    "accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.7",
    "accept-language": "uk-UA,uk;q=0.9,en-US;q=0.8,en;q=0.7",
    "cache-control": "max-age=0",
    "priority": "u=0, i",
    "sec-ch-ua": "'Chromium';v='124', 'Google Chrome';v='124', 'Not-A.Brand';v='99'",
    "sec-ch-ua-mobile": "?1",
    "sec-ch-ua-platform": "Android",
    "sec-fetch-dest": "document",
    "sec-fetch-mode": "navigate",
    "sec-fetch-site": "same-origin",
    "sec-fetch-user": "?1",
    "upgrade-insecure-requests": "1",
    "user-agent": "Mozilla/5.0 (Linux; Android 6.0; Nexus 5 Build/MRA58N) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/124.0.0.0 Mobile Safari/537.36"
  },
  "gallery_obj": {
    "attribute_name": "div",
    "attrs": {
      "class": "products__grid"
    }
  },
  "product_obj": {
    "attribute_name": "div",
    "attrs": {
      "class": "products__line"
    }
  },
  "name_obj": {
    "attribute_name": "a",
    "attrs": {
      "class": "products__line-title"
    },
    "path_to_data": [],
    "data_source": "text"
  },
  "price_obj": {
    "attribute_name": "span",
    "attrs": {
      "class": "products__line-price"
    },
    "path_to_data": [],
    "data_source": "text",
    "has_price_interval": false,
    "extract_pattern": "грн(\\d+)",
    "separator": null,
    "currency": "UAH"
  },
  "picture_obj": {
    "attribute_name": "div",
    "attrs": {
      "class": "products__line-images js-slick-products"
    },
    "path_to_data": ["a", "img"],
    "data_source": "data-lazy"
  },
  "url_obj": {
    "attribute_name": "a",
    "attrs": {
      "class": "products__line-title"
    },
    "path_to_data": [],
    "data_source": "href"
  },
  "country": "Україна"
}